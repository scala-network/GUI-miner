{
  "app_name": "BLOC GUI Miner v0.0.3",
  "environments": [
    {"arch": "amd64", "os": "linux"},
    {"arch": "amd64", "os": "darwin"},
    {
      "arch": "amd64",
      "os": "windows",
      "env": {
        "CC": "x86_64-w64-mingw32-gcc",
        "CXX": "x86_64-w64-mingw32-g++",
        "CGO_ENABLED": "1"
      }
    }
  ],
  "icon_path_darwin": "resources/icon-bloc.icns",
  "icon_path_linux": "resources/icon-bloc.png",
  "icon_path_windows": "resources/icon-bloc.ico",
  "output_path": "../bin"
}
